	"type": "module",
	"exports": {
		".": "./dist/code.js"
	},
	"scripts": {
		"build": "wireit",
		"postbuild": "wireit",
		"test": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc"
		},
		"postbuild": {
			"command": "attw --profile esm-only --pack ."
		},
		"test": {
			"command": "node --test-reporter spec --test 'src/**/*.test.ts'",
			"files": [
				"src/**/*.ts"
			],
			"output": []
		}
	},
	"dependencies": {
		"dedent": "^1.7.0",
		"type-fest": "^4.41.0"
	}
}
