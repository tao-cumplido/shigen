{
	"name": "@shigen/enum",
	"version": "0.6.0",
	"description": "Enums for JavaScript",
	"keywords": [
		"enum",
		"javascript",
		"typescript",
		"esm"
	],
	"license": "ISC",
	"author": "Tao Cumplido",
	"repository": "https://github.com/tao-cumplido/shigen/tree/main/packages/enum",
	"bugs": "https://github.com/tao-cumplido/shigen/issues",
	"type": "module",
	"exports": {
		".": "./dist/enum.js"
	},
	"scripts": {
		"build": "wireit",
		"postbuild": "wireit",
		"test": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc"
		},
		"preserve-brand-type": {
			"command": "node preserve-brand-type.mjs"
		},
		"postbuild": {
			"command": "attw --profile esm-only --pack .",
			"dependencies": [
				"preserve-brand-type"
			]
		},
		"test:types": {
			"command": "tsc --project tsconfig.test.json",
			"files": [
				"src/**/*.ts"
			],
			"output": []
		},
		"test": {
			"command": "node --test-reporter spec --test 'src/**/*.test.ts'",
			"dependencies": [
				{
					"script": "test:types",
					"cascade": false
				}
			],
			"files": [
				"src/**/*.ts"
			],
			"output": []
		}
	},
	"dependencies": {
		"type-fest": "^4.41.0"
	}
}
