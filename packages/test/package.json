{
	"name": "@shigen/test",
	"version": "0.2.1",
	"description": "Tools for testing",
	"keywords": [
		"test",
		"file",
		"system",
		"fs",
		"fixture",
		"temp",
		"async",
		"await",
		"using",
		"symbol",
		"dispose",
		"asyncdispose"
	],
	"license": "ISC",
	"author": "Tao Cumplido",
	"repository": "https://github.com/tao-cumplido/shigen/tree/main/packages/test",
	"bugs": "https://github.com/tao-cumplido/shigen/issues",
	"type": "module",
	"exports": {
		"./fs": "./dist/fs/fixture.js"
	},
	"scripts": {
		"build": "wireit",
		"postbuild": "wireit",
		"test": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc"
		},
		"postbuild": {
			"command": "attw --profile esm-only --pack ."
		},
		"test": {
			"command": "node --test-reporter spec --test 'src/**/*.test.ts'",
			"files": [
				"src/**/*.ts"
			],
			"output": []
		}
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@binary-tools/core": "0.11.0-alpha.2",
		"execa": "^9.6.0",
		"type-fest": "^4.8.3"
	},
	"peerDependencies": {
		"@types/node": ">=18.19.0"
	},
	"peerDependenciesMeta": {
		"@types/node": {
			"optional": true
		}
	}
}
